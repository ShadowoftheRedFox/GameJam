config_version=5

[application]

config/name="Game"
run/main_scene="res://scenes/UI/Main/MainMenu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
boot_splash/image="res://assets/credits/logo_sans_bg.png"
config/icon="res://icon.svg"
config/windows_native_icon="res://tests/icon.ico"
boot_splash/minimum_display_time=1000

[autoload]

Game="*res://scripts/Controllers/game_controller.gd"
Transition="*res://scripts/Controllers/transition_controller.gd"
Save="*res://scripts/Controllers/save_controller.gd"
Multi="*res://scripts/Controllers/multiplayer_controller.gd"
Server="*res://scripts/multiplayer/server.gd"
Config="*res://scripts/config/config.gd"

[display]

window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Game"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg")

